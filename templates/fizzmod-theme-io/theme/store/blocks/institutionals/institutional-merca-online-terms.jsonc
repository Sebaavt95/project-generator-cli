	"responsive-layout.tablet#merca-online-terms": {
		"children": ["flex-layout.row#institutional-header-merca-online-terms", "flex-layout.row#institutional-content-merca-online-terms"]
	},
	"responsive-layout.phone#merca-online-terms": {
