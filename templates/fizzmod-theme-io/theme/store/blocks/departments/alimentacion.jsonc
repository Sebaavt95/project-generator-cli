    }
  },
  "image#category-img--3": {
    "props": {
      "src": "https://via.placeholder.com/200x200.png/09F/FFF",
      "alt": "Frutas y Verduras",
      "link": "https://fizzmodapps.myvtex.com/alimentacion/frutas-y-verduras",
