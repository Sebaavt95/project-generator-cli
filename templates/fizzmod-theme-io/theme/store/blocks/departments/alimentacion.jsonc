{
  "store.custom#alimentacion": {
    "blocks": [
      "responsive-layout.desktop#department--2",
      "responsive-layout.tablet#department--2",
      "responsive-layout.phone#department--2"
    ]
  },
  "responsive-layout.desktop#department--2": {
    "children": [
      "flex-layout.row#department-container--2"
    ]
  },
  "flex-layout.row#department-container--2": {
    "props": {
      "blockClass": "department-container"
    },
    "children": [
      "flex-layout.col#department-content--2"
    ]
  },
  "responsive-layout.tablet#department--2": {
    "children": [
      "flex-layout.row#department-container--2"
    ]
  },
  "responsive-layout.phone#department--2": {
    "children": [
      "flex-layout.row#department-container--2"
    ]
  },
  "flex-layout.col#department-content--2": {
    "props": {
      "width": "100%"
    },
    "children": [
      "flex-layout.row#department-main-banner--2",
      "rich-text#department-title--2",
      "flex-layout.row#categories-cards--2"
    ]
  },
  "flex-layout.row#department-main-banner--2": {
    "props": {
      "blockClass": "department__main__banner__container",
      "horizontalAlign": "center",
      "fullWidth": true
    },
    "children": [
      "flex-layout.col#department-main-banner-slider--2"
    ]
  },
  "flex-layout.col#department-main-banner-slider--2": {
    "props": {
      "blockClass": "department__main__banner__slider__container",
      "fullWidth": true
    },
    "children": [
      "list-context.image-list#department-main-banner-list-images--2"
    ]
  },
  "list-context.image-list#department-main-banner-list-images--2": {
    "children": [
      "slider-layout#department-main-banner-slider--2"
    ],
    "props": {
      "images": [
        {
          "image": "https://via.placeholder.com/1239x299.png",
          "mobileImage": "https://via.placeholder.com/1024x420.png"
        },
        {
          "image": "https://via.placeholder.com/1239x299.png/09F/FFF",
          "mobileImage": "https://via.placeholder.com/1024x420.png/09F/FFF"
        },
        {
          "image": "https://via.placeholder.com/1239x299.png/08A/FFF",
          "mobileImage": "https://via.placeholder.com/1024x420.png/08A/FFF"
        }
      ]
    }
  },
  "slider-layout#department-main-banner-slider--2": {
    "props": {
      "blockClass": "department__main__banner__slider",
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "showNavigationArrows": "never",
      "showPaginationDots": "always",
      "infinite": true
    }
  },
  "rich-text#department-title--2": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "### Alimentación",
      "blockClass": "department-title"
    }
  },
  "flex-layout.row#categories-cards--2": {
    "children": [
      "flex-layout.col#category-card--3",
      "flex-layout.col#category-card--19",
      "flex-layout.col#category-card--20",
      "flex-layout.col#category-card--21"
    ],
    "props": {
      "horizontalAlign": "left",
      "colSizing": "auto",
      "blockClass": "categories-cards-container"
    }
  },
  "flex-layout.col#category-card--3": {
    "children": [
      "flex-layout.col#category-card-top--3",
      "flex-layout.col#category-card-bottom--3"
    ],
    "props": {
      "blockClass": "department-page-category-card"
    }
  },
  "flex-layout.col#category-card-top--3": {
